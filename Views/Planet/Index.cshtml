@inject App.Services.PlanetService planetService

<h1>Cac hanh tinh trong he mat troi</h1>

<div class="list-group">
    @foreach (var planet in planetService)
{
    <a class="list-group-item list-group-item-action" asp-action="@planet.Name" asp-controller="Planet">
        @planet.VnName - @planet.Name
    </a>
}   
</div>